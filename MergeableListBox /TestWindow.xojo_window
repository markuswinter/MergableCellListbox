	#tag Event
		Sub Open()
		  ' make selected versions of picture
		  
		  Dim MaskPic as New Picture(IMG_0441.Width, IMG_0441.Height, 32)
		  MaskPic.Graphics.ForeColor = me.BackgroundColor(True, True)
		  MaskPic.Graphics.FillRect 0, 0, IMG_0441.Width, IMG_0441.Height
		  MaskPic.Mask.Graphics.ForeColor = &c6B6B6B
		  MaskPic.Mask.Graphics.FillRect 0, 0, IMG_0441.Width, IMG_0441.Height
		  
		  SelectedActivePic= New Picture(IMG_0441.Width, IMG_0441.Height, 32)
		  SelectedActivePic.Graphics.DrawPicture IMG_0441, 0,0
		  SelectedActivePic.Graphics.DrawPicture MaskPic, 0, 0
		  
		  MaskPic.Graphics.ForeColor = me.BackgroundColor(True, False)
		  MaskPic.Graphics.FillRect 0, 0, IMG_0441.Width, IMG_0441.Height
		  
		  SelectedInActivePic= New Picture(IMG_0441.Width, IMG_0441.Height, 32)
		  SelectedInActivePic.Graphics.DrawPicture IMG_0441, 0,0
		  SelectedInActivePic.Graphics.DrawPicture MaskPic, 0, 0
		  
		  
		  'Setup the line Spacing popup
		  
		  If me.WrapByDefault Then
		    LnSpace.ListIndex = 1
		  Else
		    LnSpace.ListIndex = 0
		  End if
		  
		  #if TargetLinux ' drawing node lines does not work properly on Linux for some unknown reason
		    me.DrawNodeLines = false
		  #endif
		  
		  'some giberish text for testing line wraping no mormal and merged cells
		  
		  Dim S As String = "Line 1"  + EndOfLine + "k45t6j34.htgigi4yuurrjjf"  + EndOfLine + "     Line2 this is a test of string wrapping"_
		  + " this is a test of string wrapping. this is a test of string wrapping"+ EndOfLine + "    Line3"
		  S = S +EndOfLine+EndOfLine+ S
		  
		  'First column used to test decimal alignment
		  me.ColumnAlignment(0) = me.AlignDecimal
		  
		  For row as integer = 0 to 100
		    me.AddRow Str(row)+".00"
		    me.CellBold(me.LastIndex,0) = True
		    For col as Integer = 1 to me.ColumnCount -1
		      me.Cell(row,col) = S
		    Next
		  Next
		  
		  me.Cell(2,3) = "A Picture Spanning Cells!!!!!"
		  me.MergeCells(2,3,4,3) ' merging cell for picture display. Picture is drawn in CellBackgroundPaint
		  
		  
		  'More merged cells some with checkboxes and some editable for testing
		  me.MergeCells(7,4,2,3)
		  
		  me.MergeCells(3,1,2,1)
		  me.CellType(7,4) = me.TypeCheckbox
		  
		  me.MergeCells(50,7,4,3)
		  me.ColumnType(0) = me.TypeCheckbox
		  
		  me.ColumnType(3) = me.TypeEditableTextArea
		  
		  
		  me.CellType(6,4) = me.TypeCheckbox
		  
		  
		End Sub
	#tag EndEvent
	#tag Event
		Function CellTextPaint(g as graphics, row as integer, column as integer, x as integer, y as Integer,  isSelected As Boolean,  ByRef VertAlign As Integer, ByRef VOffset As integer, ByRef LineSpacing as integer) As Boolean
		  
		  If row < 2 then
		    LineSpacing = -1
		  Else
		    LineSpacing = LnSpace.ListIndex -1
		  End if
		  
		  VertAlign = Self.VertAlign.ListIndex
		  VOffset = Val(VertOffset.List(VertOffset.ListIndex))
		  
		  If  row = 2 and column = 3 Then
		    
		    g.PenWidth = 2
		    g.PenHeight = 2
		    g.ForeColor = &cFEFF42
		    g.DrawOval 3,3, g.Width-6,g.Height -6
		    g.Bold = True
		    g.ForeColor = &cFFFB02
		    g.TextSize = 16
		  End if
		  
		End Function
	#tag EndEvent
	#tag Event
		Sub EnableMenuItems()
		  If me.SelCount > 0 Then
		    RowMenu.Enable
		    RowInsert.Enable
		    RowRemove.Enable
		  End If
		End Sub
	#tag EndEvent
	#tag Event
		Sub CellGotFocus(row as Integer, column as Integer)
		  If  row = 2 and column = 3 Then
		    me.ActiveCell.BackColor = &c41813F
		    me.ActiveCell.Bold = True
		    me.ActiveCell.TextColor = &cFFFB02
		    me.ActiveCell.TextSize = 16
		    
		  Else
		    me.ActiveCell.BackColor = &cFFEAD8
		  End if
		  
		  
		  
		End Sub
	#tag EndEvent
	#tag Event
		Function CellBackgroundPaint(g as graphics,row as integer, column as integer,isSelected as Boolean) As boolean
		  If  row = 10 and column = 0 AND Not isSelected Then 
		    'colored backgroud... As folder Button will insert folder here then merged cells,
		    ' Color should be in whole merged area as thsi will be top right cell
		    g.ForeColor = &cFFFBD3
		  End If
		  
		  If  row = 2 and column = 3 THen
		    If me.SelectedCell(row,column) Then
		      If me.IsActive Then
		        g.DrawPicture SelectedActivePic, 0,0,g.Width, g.Height,0,0,IMG_0441.Width, IMG_0441.Height
		      Else
		        g.DrawPicture SelectedInActivePic, 0,0,g.Width, g.Height,0,0,IMG_0441.Width, IMG_0441.Height
		      End if
		    Else ' draw a picture in the MergedCells if not Selected
		      g.DrawPicture IMG_0441, 0,0,g.Width, g.Height,0,0,IMG_0441.Width, IMG_0441.Height
		      
		    End if
		    Return true
		  End if
		End Function
	#tag EndEvent
	#tag Event
		Sub ExpandRow(row As integer)
		  Static Count as Integer
		  if me.RowDepth(row) = 0 then Count = 0
		  If Count > 13 Then Return
		  Dim ChildRow As Integer
		  
		  Count = Count + 1
		  me.AddFolder  ""
		  ChildRow = me.LastIndex
		  
		  Me.MergeCells(ChildRow,0,1,4)
		  Me.CellAlignment(ChildRow,0) = me.AlignLeft
		  me.Cell(ChildRow,0) = "Folder Added in ExpandRow: " + Str(Count)
		  
		  Count = Count + 1
		  me.AddFolder  ""
		  ChildRow = me.LastIndex
		  me.AddRow ""
		  Me.MergeCells(ChildRow,0,2,6)
		  Me.CellAlignment(ChildRow,0) = me.AlignLeft
		  me.Cell(ChildRow,0) = "Second Folder Added in ExpandRow: " + Str(Count)
		  
		  ' The next two rows will be merged... ChildRow is top row for mergedcells
		  Count = Count + 1
		  me.AddRow ""
		  ChildRow = me.LastIndex
		  me.AddRow ""
		  
		  Me.MergeCells(ChildRow,0,2,5)
		  Me.CellAlignment(ChildRow,0) = me.AlignLeft
		  me.Cell(ChildRow,0) = "Child Added in ExpandRow: " + Str(Count)
		  
		  if Count < 6 then me.Expanded(row+2) = True
		  
		  
		End Sub
	#tag EndEvent
	#tag Event
		Function CellClick(row as Integer, column as Integer, x as Integer, y as Integer) As Boolean
		  If Row = 7 And column = 4 and me.SelectedCell(row, column) Then
		    me.EditCell(7,4)
		    Return True
		  End if
		End Function
	#tag EndEvent
	#tag Event
		Function EditingMode(row as Integer, column as Integer) As EditMode
		  If Row=7 and column = 4 Then
		    Return EditMode.WithVerticalScrollBar
		  ElseIf Row = 8 and column = 3 Then
		    Return EditMode.Custom
		  ElseIf Row = 10 and column = 3 Then
		    Return EditMode.WithVerticalScrollBar
		  End If
		End Function
