#tag WindowCode
	#tag Event
		Sub Open()
		  For col as Integer = 1 to Listbox1.ColumnCount -1
		    Listbox1.ColumnAlignment(col) = me.HorizAlign.ListIndex
		  Next
		  
		  
		End Sub
	#tag EndEvent


	#tag MenuHandler
		Function RowInsert() As Boolean Handles RowInsert.Action
			
			Listbox1.InsertRow(Listbox1.ListIndex)
			Return True
		End Function
	#tag EndMenuHandler

	#tag MenuHandler
		Function RowRemove() As Boolean Handles RowRemove.Action
			Dim Found As Integer = -1
			Dim ubSelected as Integer = Listbox1.SelCount -1
			Dim SelectedList() as Integer
			ReDim SelectedList(ubSelected)
			Dim row as integer = Listbox1.ListIndex
			
			Do
			If Listbox1.Selected(row) Then
			Found = Found +1
			SelectedList(Found) = row
			End If
			Row = row + 1
			Loop Until Found = ubSelected
			 
			For row = ubSelected Downto 0
			Listbox1.RemoveRow(SelectedList(row))
			Next
			
			Return True
			
		End Function
	#tag EndMenuHandler


